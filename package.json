{
  "name": "kaios-parcel-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "npm run clean && export BROWSERSLIST_ENV=kaios2 && parcel build ./src/**/*.html --no-source-maps --no-cache --target kaios2",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garredow/foxcasts-lite.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jsqr": "^1.4.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.3.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "parcel": "^2.3.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "rimraf": "^3.0.2"
  },
  "staticFiles": {
    "staticPath": "public"
  },
  "browserslist": {
    "kaios2": "Firefox 48",
    "kaios3": "Firefox 84"
  },
  "targets": {
    "kaios2": {
      "distDir": "./dist"
    },
    "kaios3": {
      "source": "src/index.html",
      "distDir": "./dist"
    }
  }
}
